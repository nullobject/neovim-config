{
  "function": {
    "prefix": "fun",
    "body": "function ${1:name}($2)$3 {\n\t$0\n}",
    "description": "Creates a function"
  },
  "exportFunction": {
    "prefix": "expfun",
    "body": "export function ${1:name}($2)$3 {\n\t$0\n}",
    "description": "Creates an exported function"
  },
  "arrowFunction": {
    "prefix": "afun",
    "body": "const ${1:name} = ($2)$3 => {\n\t$0\n}",
    "description": "Creates an arrow function"
  },
  "exportArrowFunction": {
    "prefix": "expafun",
    "body": "export const ${1:name} = ($2)$3 => {\n\t$0\n}",
    "description": "Creates an exported arrow function"
  },
  "describeBlock": {
    "prefix": "desc",
    "body": "describe(\"$1\", () => {\n\tit(\"$2\", () => {\n\t\t${0:expect(true).toBe(true)}\n\t})\n})",
    "description": "Creates a describe/it block"
  },
  "itBlock": {
    "prefix": "it",
    "body": "it(\"${2:does something}\", () => {\n\t${0:expect(true).toBe(true)}\n})",
    "description": "Creates an it block"
  }
}
